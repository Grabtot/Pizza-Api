{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Logs": {
    "WriteInFile": false
  },
  "IdentityOptions": {
    "PasswordOptions": {
      "RequiredLength": 3,
      "RequiredUniqueChars": 1,
      "RequireNonAlphanumeric": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireDigit": false
    },
    "SignInOptions": {
      "RequireConfirmedEmail": false,
      "RequireConfirmedPhoneNumber": false,
      "RequireConfirmedAccount": false
    }
  },
  "EmailOptions": {
    "UseEmailSender": true,
    "Host": "localhost",
    "Port": "25"
  },
  "SeedDatabase": true,
  "ClientAppOptions": {
    "UseClientUrisInEmails": false,
    "BaseUrl": "https://localhost:7172",
    "ConfirmationPath": "account/confirmEmail",
    "PasswordResetPath": "account/resetPassword"
  }
}